{
    "version": 2,
    "builds": [
        {
            "src": "*.py",
            "use": "@liudonghua123/now-flask",
            "env": {
                "PIP_VERSION": "latest"
              }
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "app.py"
        }
    ]
  }